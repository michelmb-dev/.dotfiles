! ===============================
!       urxvt
! ===============================

! special
URxvt.background: #24273A
URxvt.foreground: #CAD3F5
URxvt.cursorColor: #A5ADCB

! black
URxvt.color0: #494D64
URxvt.color8: #5B6078

! red
URxvt.color1: #ED8796
URxvt.color9: #ED8796

! green
URxvt.color2: #A6DA95
URxvt.color10: #A6DA95

! yellow
URxvt.color3: #EED49F
URxvt.color11: #EED49F

! blue
URxvt.color4: #8AADF4
URxvt.color12: #8AADF4

! magenta
URxvt.color5: #F5BDE6
URxvt.color13: #F5BDE6

! cyan
URxvt.color6: #8BD5CA
URxvt.color14: #8BD5CA

! white
URxvt.color7: #B8C0E0
URxvt.color15: #A5ADCB


URxvt.letterSpace:      0
URxvt.lineSpace:				0
URxvt.geometry:					120x50
URxvt.internalBorder:   12
URxvt.cursorBlink:      true 
URxvt.cursorUnderline:  true
URxvt.scrollBar:        false
URxvt.scrollBar_right:  false
URxvt.urgentOnBell:     false
URxvt.depth:            24
URxvt*utf8:	            true
URxvt.iso14755:         false
URxvt.iso14755_52:      false

URxvt*font:            		xft:Iosevka Mayukai Monolite:style=Regular:size=12
URxvt*boldFont:        		xft:Iosevka Mayukai Monolite:style=Bold:size=12
URxvt*italicFont:        	xft:Iosevka Mayukai Monolite:style=Italic:size=12
URxvt*boldItalicFont:     xft:Iosevka Mayukai Monolite:style=Bold Italic:size=12

! URxvt*font:             xft:Menlo for Powerline:regular:size=11
! URxvt*boldFont:         xft:Menlo for Powerline:bold:size=11

! URxvt*font:             xft:Droid Sans Mono Nerd Font:regular:size=14
! URxvt*boldFont:         xft:Droid Sans Mono Nerd Font:bold:size=14

! URxvt*font:             xft:Fira Code:regular:size=12,
! URxvt*boldFont:         xft:Fira Code:bold:size=12

URxvt.perl-ext-common:  default,matcher,resize-font
URxvt.url-launcher:     google-chrome-stable
URxvt.matcher.button:   2

URxvt.keysym.C-Up:      resize-font:bigger
URxvt.keysym.C-Down:    resize-font:smaller

! ===============================
! Xft
! ===============================
Xft.hinting:            1
Xft.hintstyle:          hintslight
Xft.antialias:          true
Xft.rgba:               subpixel
Xft.dpi:                96

